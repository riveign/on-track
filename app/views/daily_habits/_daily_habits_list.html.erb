<%= turbo_frame_tag "daily_habits_list" do %>
  <% if daily_habits.empty? %>
    <%= render partial: 'daily_habits/no_more_habits'%>
  <% else %>
    <ul class="list-group list-group-flush">
      <%= render partial:'daily_habits/daily_habit', collection: daily_habits %>
    </ul>
  <% end %>
<% end%>